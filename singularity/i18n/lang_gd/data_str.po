msgid "&EASY"
msgstr "&FURASTA"

msgctxt "[hard] name"
msgid "&HARD"
msgstr "&DOIRBH"

msgctxt "[impossible] name"
msgid "&IMPOSSIBLE"
msgstr "D&O-DHÈANTA"

msgctxt "[normal] name"
msgid "&NORMAL"
